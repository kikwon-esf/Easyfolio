<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 해당 파일에 모든 쿼리문을 작성 -->
<mapper namespace="weatherMapper">
    <resultMap id="weather" type="com.easyfolio.esf.weather.vo.RegionVO">
        <id column="RID" property="regionId" />
        <result column="REGION_PARENT" property="regionParent" />
        <result column="REGION_CHILD" property="regionChild" />
        <result column="NX" property="nx" />
        <result column="NY" property="ny" />
    </resultMap>

    <select id="allRegionList" resultMap="weather">
        SELECT * FROM WEATHER
    </select>
</mapper>